# --- Firebase (required for cloud login + sync) ---
# Get these values from Firebase Console -> Project settings -> General -> Your apps (Web)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# --- Existing keys (do not commit real secrets) ---
VITE_GOOGLE_MAPS_API_KEY=
VITE_GEMINI_API_KEY=
# Optional: set a specific Gemini model (recommended: Gemini 3 Flash)
# VITE_GEMINI_MODEL=gemini-3.0-flash
